package chap05.array;

public class ArrayExam02 {

	public static void main(String[] args) 
	{
		int [] arr = {10,20,30,40,50};
		int sum=0;
		
		for(int i=0; i<arr.length; i++)
		{
			
			System.out.println("Array"+i+": "+arr[i]);
			
		}
		//sum = arr[i];
		System.out.println(arr[i]/arr.length);
	}

}
