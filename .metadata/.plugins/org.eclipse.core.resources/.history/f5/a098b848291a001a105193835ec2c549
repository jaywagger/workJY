package chap05.array;

public class ArrayCopyTest01 {

	public static void main(String[] args) 
	{
		int[] firstArr	= new int [5];
		
		//배열의 값을 할당하기 위한 for문
		for(int i=0; i<firstArr.length; i++)
		{
			firstArr[i] = 10+i;
			
		}
		//배열에 저장된 값을 출력하기위한 for문
		for(int i=0; i<firstArr.length; i++)
		{
			System.out.println(firstArr[i]);
			
		}
	}

}
