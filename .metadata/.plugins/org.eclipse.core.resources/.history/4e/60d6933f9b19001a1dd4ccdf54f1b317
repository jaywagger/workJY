package chap02.variable;
//변수에 지정된 값을 바꿔주기
public class VarEx3_Constant {

	public static void main(String[] args) 
	{
		final int x = 10;
		final int y = 20;
		int tmp = 0;
			
		System.out.println("X:"+ x + " Y:" + y);
			
		tmp = x; //tmp->x, x->y, y->tmp
		x = y;
		y = tmp;
		
		System.out.println("X:"+ x + " Y:" + y);
		
		

	}

}
