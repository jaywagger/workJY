package chap04;

import java.util.Random;
//if문과 랜덤 클래스를 연습하는 문제

public class IFTest01 {
	public static void main(String[] args) {
		
		Random r = new Random();
		int i = r.nextInt(100)+1;
		
		System.out.println("숫자는 "+i);
		System.out.println();
		
		if(i>=50)
		{
			System.out.println("50 이상");
		}
		else 
		{
			System.out.println("50 미만");
		}
		
	}

}
